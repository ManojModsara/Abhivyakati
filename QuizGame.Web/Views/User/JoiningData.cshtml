@{
    ViewBag.Title = "JoiningData";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/datatables/css", "~/Content/css/bootstrap-switch.css", "~/Scripts/plugins/alertifyjs/css/alertify.min.css")

<section class="content-header">
    <h1>
        Joining Data
    </h1>
</section>
<section>
    <div class="row">
        <div class="form-group col-md-2">
            <label class="control-label">Name</label>
            <input type="text" id="txtName" value="" class="form-control" />
        </div>
        <div class="form-group col-md-2">
            <label class="control-label">UniqueId</label>
            <input type="text" id="txtUniqueId" value="" class="form-control" />
        </div>
        <div class="form-group col-md-1">
            <br />
            <input type="button" value="Search" id="btnSearch" class="btn btn-success" />
        </div>
    </div>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            @Html.Partial("_Notification")
            <div class="box">
                <div class="box-body">
                    <div class="table-responsive scroll_tbl">
                        <table id="grid-index" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>Id</th>
                                    <th>Name</th>
                                    <th>Father/Husband Name</th>
                                    <th>MobileNumber</th>
                                    <th>AlternateMobileNumber</th>
                                    <th>DOB</th>
                                    <th>District</th>
                                    <th>Block</th>
                                    <th>GramPanchayat</th>
                                    <th>Village</th>
                                    <th>PinCode</th>
                                    <th>Education</th>
                                    <th>FullAddress</th>
                                    <th>AadharNumber</th>
                                    <th>PanNumber</th>
                                    <th>UniqueId</th>
                                    <th>JoiningTime</th>
                                    <th>Actions</th>

                                </tr>
                            </thead>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@section scripts{
    @Scripts.Render("~/bundles/datatables", "~/bundles/jqueryval", "~/bundles/datepicker")
    @Scripts.Render("~/Scripts/plugins/alertifyjs/alertify.min.js")
    @Scripts.Render("~/Scripts/Custom/adminuser/joiningdata.js?ver=1.01")
}

