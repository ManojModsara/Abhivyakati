
@using QuizGame.Core;
@using QuizGame.Web.Code.LIBS;
@using QuizGame.Web.Models.Others;


@{
    ViewBag.Title = "User - StoreAid";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@Styles.Render("~/datatables/css",  "~/Scripts/plugins/alertifyjs/css/alertify.min.css", "~/Content/select2")

<section class="content-header">
    <h1>
        @*@QuizGame.Resource.RoleRes.PageHeading*@

    </h1>
</section>
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            @Html.Partial("_Notification")
            <div class="box">
                <div class="box-header">
                    <div class="btn-group">
                        @*@(Html.Raw(Html.InsertButton(Url.Action("createedit", "adminrole"), "modal-add-edit-adminrole")))*@
                    </div>
                </div>
                <div class="box-body">

                    <table id="grid-index" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Role Name</th>
                                <th>Actions</th>

                            </tr>
                        </thead>
                    </table>

                </div>
            </div>
        </div>
    </div>
</section>

@Html.Partial("_Modal", new Modal { ID = "modal-add-edit-adminrole", AreaLabeledId = "modal-add-edit-adminrole-label", Size = Enums.ModalSize.Medium })
@Html.Partial("_Modal", new Modal { ID = "modal-delete-adminrole", AreaLabeledId = "modal-delete-adminrole-label", Size =Enums.ModalSize.Medium })

@section scripts{
    @Scripts.Render("~/bundles/datatables", "~/bundles/jqueryval", "~/bundles/datepicker", "~/bundles/select2")
    @Scripts.Render("~/Scripts/plugins/alertifyjs/alertify.min.js")
    @Scripts.Render("~/Scripts/Custom//AdminRole/index.js")

}




