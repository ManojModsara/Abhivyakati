
@using QuizGame.Web.Code.LIBS

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  - Excel Data</title>
    @*@Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@


    @Styles.Render("~/Content/css/bootstrap.min.css",
"~/Content/css/font-awesome.min.css",
"~/Content/css/AdminLTE.min.css",
"~/Content/css/skins/_all-skins.min.css", "~/Content/plugins/iCheck/square/blue.css",
"~/Content/css/Custom.css"
)
    <style>
        .navbar-inverse {
            background-color: #204d74;
            border-color: #122b40;
        }

            .navbar-inverse .navbar-nav > li > a {
                color: whitesmoke;
                font-weight: bold;
                text-transform: uppercase;
            }

        .body-content {
            margin-top: -20px;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
        }

        .site-footer {
            width: 100%;
            min-height: 170px;
            height: auto !important;
            height: 170px;
            margin-top: 5px;
            text-align: center;
            background-color: #204d74;
            border-color: #122b40;
            box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
            height: auto;
        }
    </style>

    @RenderSection("styles", false)
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", false)
</head>
<body style="background-color: #e5e5e5; height:100%">
    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            @*<a href="@Url.Action("Index")" class="navbar-brand">

                <span style="color:navajowhite"><b style="color:white">Excel </b>DataFilter</span>
            </a>*@
            <div class="navbar-header">

                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                    <span id="myappname" class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            @{Html.RenderAction("adminmenu", "account");}
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()

    </div>



    <footer class="site-footer">
        <p style="color:white;">&copy; @DateTime.Now.Year </p>
    </footer>


    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@

    @Scripts.Render("~/Scripts/jquery-3.4.1.js", "~/Scripts/bootstrap.min.js", "~/Content/plugins/iCheck/icheck.min.js", "~/Scripts/global.js")

    @RenderSection("scripts", required: false)


    <script type="text/javascript">
        Global.DomainName = '@SiteKey.DomainName';

        //$("li").find().attr('class', 'active').parents().find("li").addClass("active");
    </script>

</body>
</html>