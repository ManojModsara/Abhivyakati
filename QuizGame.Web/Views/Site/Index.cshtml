            <button class="slider-dot w-3 h-3 sm:w-4 sm:h-4 rounded-full bg-white/30 hover:bg-white transition-all duration-300"
                    data-slide="3"></button>
        </div>
    </div>

    <!-- Slider Arrows -->
    <button class="absolute left-2 sm:left-4 lg:left-8 top-1/2 transform -translate-y-1/2 z-20 w-10 h-10 sm:w-12 sm:h-12 bg-white/20 backdrop-blur-md rounded-full flex items-center justify-center text-white hover:bg-white/30 transition-all duration-300"
